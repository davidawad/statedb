     ] 
   }, 

    { "sec": "Sec. 08.88.051.  - Commission meetings and officers.", 
      "index":   8, 
      "link":   "/codes/alaska/2000/title-08/chapter-08-88/sec-08-88-051/", 
      "raw":  [ 

        "(a) The commission shall hold a regular annual meeting. It may hold a special meeting at the call of the chairman or at the request of three commission members.",
        "(b) At least two judicial districts shall be represented and at least a majority of the commission members shall be present in order to conduct business.",
        "(c) The commission shall elect its officers at the first meeting of each fiscal year.",
     ] 
   }, 

    { "sec": "Sec. 08.88.060.  - [Repealed, Sec. 3 ch 95 SLA 1964].", 
      "index":   8, 
      "link":   "/codes/alaska/2000/title-08/chapter-08-88/sec-08-88-060/", 
      "raw":  [ 

        "Repealed or Renumbered",
     ] 
